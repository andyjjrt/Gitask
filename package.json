{
  "name": "gitask",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite --port 3000",
    "build:frontend": "tsc && vite build",
    "dev:backend": "nodemon --watch './api' --watch '.env' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' api/index.ts",
    "dev": "concurrently --kill-others \"npm run dev:frontend\" \"npm run dev:backend\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@szhsin/react-menu": "^3.5.2",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "h3": "^1.5.0",
    "node-fetch": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-redux": "^8.0.5",
    "react-router": "^6.8.2",
    "react-router-dom": "^6.8.2"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-modal": "^3.13.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.21",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.0"
  }
}
